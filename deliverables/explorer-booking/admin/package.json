{
  "name": "explorer-booking-admin",
  "version": "1.0.0",
  "description": "Admin panel for Explorer Shack booking system",
  "main": "index.html",
  "scripts": {
    "start": "http-server . -p 3002 -c-1",
    "dev": "http-server . -p 3002 -c-1 -o",
    "build": "npm run optimize && npm run bundle",
    "optimize": "echo 'Optimizing admin assets...'",
    "bundle": "echo 'Bundling admin files...'",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "serve": "python3 -m http.server 3002"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "chart.js": "^4.4.0"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "admin",
    "booking",
    "management",
    "dashboard",
    "analytics"
  ],
  "author": "Explorer Shack Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/explorershack/booking-system.git"
  },
  "bugs": {
    "url": "https://github.com/explorershack/booking-system/issues"
  },
  "homepage": "https://github.com/explorershack/booking-system#readme"
}
